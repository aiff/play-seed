    "del": "1.2.0",
    "gulp": "3.9.0",
    "gulp-browserify": "0.5.1",
    "gulp-concat": "2.6.0",
    "gulp-if": "1.2.5",
    "gulp-minify-css": "1.2.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.3",
    "gulp-sass": "2.0.4",
    "gulp-twig": "0.3.0",
    "gulp-uglify": "1.2.0",
