{
  "name": "play-seed",
  "version": "0.0.1",
  "description": "play-seed assets(js, css, img, font)",
  "repository": "https://github.com/yangbajing/play-seed",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "build": "./node_modules/.bin/gulp clean && ./node_modules/.bin/gulp build",
